    "turbo": "^2.6.1",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2",
