{
  "name": "@workspace/kas-data",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": "./src/*.ts"
  },
  "scripts": {
    "lint": "eslint src scripts --max-warnings 0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:scripts": "tsc --project tsconfig.node.json --noEmit",
    "fetch-data": "tsx scripts/fetch_kas.ts --out data"
  },
  "dependencies": {
    "@workspace/chart-utils": "workspace:*"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "^9.39.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "license": "AGPL-3.0-only"
}
