{
  "name": "@workspace/kas-data",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": "./src/*.ts"
  },
  "scripts": {
    "lint": "eslint src scripts --max-warnings 0",
    "typecheck": "tsc --noEmit -p tsconfig.app.json && tsc --noEmit -p tsconfig.node.json",
    "fetch-data": "tsx scripts/fetch_kas.ts --out data",
    "stub-data": "tsx scripts/stub_data.ts --dir data"
  },
  "dependencies": {
    "@workspace/utils": "workspace:*"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "^9.39.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "license": "AGPL-3.0-only"
}
