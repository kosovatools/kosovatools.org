{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node Scripts",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "lib": ["ES2022"],
    "noEmit": true,
    "types": ["node"]
  }
}
